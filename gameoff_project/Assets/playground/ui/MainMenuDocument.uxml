            <ui:VisualElement name="btns-container" style="height: 30%; background-color: rgb(255, 0, 0); border-left-color: rgba(0, 0, 0, 0); border-right-color: rgba(0, 0, 0, 0); border-top-color: rgba(0, 0, 0, 0); border-bottom-color: rgba(0, 0, 0, 0); align-items: center; justify-content: center; width: 30%; margin-left: 1%; margin-right: 1%; margin-top: 1%; margin-bottom: 1%;">
                <ui:Button text="Start" display-tooltip-when-elided="true" name="start-btn" class="main-menu-btn-style" />
                <ui:Button text="Quit" display-tooltip-when-elided="true" name="quit-btn" class="main-menu-btn-style" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
